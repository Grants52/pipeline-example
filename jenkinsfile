pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Code Deployed..'
            }
        stage('Run job') {
            steps {
                build job: 'helloworld_job'
            }
        }
    }
}
